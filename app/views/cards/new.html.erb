<% title "Create New Card" %>
<h1>New card</h1>

<% form_for(@card, :url => project_cards_path(@card.project_id)) do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form", :locals => {:form => f} %>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', project_path(@card.project_id) %>